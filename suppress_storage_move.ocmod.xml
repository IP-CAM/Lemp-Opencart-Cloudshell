<?xml version="1.0" encoding="utf-8"?>
<modification>
  <name>Suppress 'Move Storage Folder' notification</name>
  <code>suppress_storage_move</code>
  <version>1.0</version>
  <author>oktvn</author>
  <link>https://github.com/oktvn</link>
  <file path="admin/controller/common/security.php">
    <operation>
      <search regex="false">
        <![CDATA[public function index() {]]>
      </search>
      <add position="after">
      return;
      </add>
    </operation>
  </file>
</modification>
